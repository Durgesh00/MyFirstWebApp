


<h1>Welcome</h1>

<% if logged_in? %>   
  <p><%= notice %> </p>

  <h1>You are Logged In, <%= current_user.email %></h1>
  <%= button_to "Rides", '/rides', method: :get%>
  <%= button_to "Logout", logout_path, method: :delete %>
 
<% else %>
  <%= button_to "Sign Up", '/users/new', method: :get%>
  <%= button_to "Login", new_login_path(), method: :get%>
<%end%>